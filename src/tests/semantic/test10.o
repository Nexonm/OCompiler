// Name: Print Test
// Description: Tests the Printer class for various types.
// Expected result: pass
// Expected fail stage: none

class PrintTest is
    method run() is
        var p : Printer()

        // Test integer printing
        var i : Integer(42)
        p.print(i)
        p.println(i)

        // Test real printing
        var r : Real(3.14)
        p.print(r)
        p.println(r)

        // Test boolean printing
        var b : Boolean(true)
        p.print(b)
        p.println(b)
        
        var b2 : Boolean(false)
        p.print(b2)
        p.println(b2)
    end

    this() is
    end
end


