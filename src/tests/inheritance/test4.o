// Name: Inheritance (method overwriting)
// Description: Child class inherits and overwrites parent method.
// Expected result: pass
// Expected fail stage: none

class Value is
    var value : Integer(10)

    method getValue() : Integer is
        return value
    end

    this() is
    end
end

class DoubleValue extends Value is

    method getValue() : Integer is
        return value.Mult(Integer(2))
    end

    this() is
    end
end